{
    "name": "vscode-gnu-global",
    "displayName": "Visual Studio Code GNU GLOBAL",
    "description": "GNU GLOBAL",
    "version": "0.0.1",
    "publisher": "jaycetyle",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:cpp",
        "onLanguage:c"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.showGlobalVersion",
            "title": "global: Show GNU GLOBAL Binary Version"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}